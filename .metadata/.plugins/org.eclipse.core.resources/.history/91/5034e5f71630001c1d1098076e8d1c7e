package api_crud;

public class UserDTO (String name, String job, String date) {
	private String name;
	private String job;
	private String date;
	
	this.setName(name);
	this.setJob(job);
	this.setDate(date);
	



	public setDados(String name, String job, String date) {
		this.setName(name);
		this.setJob(job);
		this.setDate(date);
	}
}

	