package api_crud;

public class UserDTO {
	private String name;
	private String job;
	private String date;
	public String getName() {
		return name;
	}
	public void Usuario(String name, String job, String date) {
		this.name = name;
		this.setJob(job);
		this.date = date;
	}
	public String getJob() {
		return job;
	}
	public void setJob(String job) {
		this.job = job;
	}	
}
