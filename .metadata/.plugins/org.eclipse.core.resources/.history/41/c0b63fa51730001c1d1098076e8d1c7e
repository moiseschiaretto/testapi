package api_crud;

public class UserDTO {
	private String name;
	private String job;
	private String date;
	
	public void setDados (String name, String job, String updateAt) {
		this.name = name;
		this.job = job;
		this.updateAt = updateAt;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}

	